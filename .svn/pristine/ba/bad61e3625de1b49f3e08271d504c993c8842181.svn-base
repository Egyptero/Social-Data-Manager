var tenantsData;

function findTenant(tenantId) {
	tenant = null;
	$(tenantsData).each(function(index) {
		if(tenantsData[index].id == tenantId)
			tenant = tenantsData[index];
	});
	return tenant;
}

function updateTenantEmails(tenantId){
	console.log("Update tenant email for tenant with id:" + tenantId);
	emails = $("#notificationEmail"+tenantId).val();
	if(emails == null || emails == "undefined") {
		alert("Please set the emails first");
		$("#notificationEmail"+tenantId).focus();
		return;
	}
	tenant = findTenant(tenantId);
	if(tenant == null){
		alert("Internal error occured , please reload page");
		return;
	}
	tenant.notificationList = emails;

	var url = SERVER_URL + "/InstGenesys/rest/InstRest/UpdateVer13";
	$.ajax({
		type : 'post',
		dataType : 'json',
		// async: false,
		contentType : 'application/json; charset=utf-8',
		data : JSON.stringify(tenant),
		url : url,
		success : function(data) {
			if (data != null) {
				if (data.error) {// Error Result
					alert("Server error:"+data.errorMessage);
				} else {
					alert("Tenant for user:"+tenant.user+" has been updated successfully for the page:"+tenant.page);
				}
			}
		}

	});
	
	//Start Tenant Update Process.
}

function updateTenantData(tenantId){
	console.log("Update tenant data for tenant with id:" + tenantId);
	tenant = findTenant(tenantId);
	if(tenant == null){
		alert("Internal error occured , please reload page");
		return;
	}
	tenant.notificationList = $("#notificationEmail"+tenantId).val();
	tenant.page = $("#pageId"+tenantId).val();
	tenant.rate = $("#fetchRate"+tenantId).val();
	tenant.queueName = $("#queueName"+tenantId).val();
	tenant.endPoint = $("#endPoint"+tenantId).val();
	tenant.mediaType = $("#mediaType"+tenantId).val();
	tenant.tenantId = $("#tenantId"+tenantId).val();
	tenant.startTime = $("#startTime"+tenantId).val();
	tenant.endTime = $("#endTime"+tenantId).val();
	tenant.startDate = $("#startDate"+tenantId).val();
	

	var url = SERVER_URL + "/InstGenesys/rest/InstRest/UpdateVer13";
	$.ajax({
		type : 'post',
		dataType : 'json',
		// async: false,
		contentType : 'application/json; charset=utf-8',
		data : JSON.stringify(tenant),
		url : url,
		success : function(data) {
			if (data != null) {
				if (data.error) {// Error Result
					alert("Server error:"+data.errorMessage);
				} else {
					alert("Tenant for user:"+tenant.user+" has been updated successfully for the page:"+tenant.page);
				}
			}
		}

	});
}

function removeTenant(tenantId) {
	console.log("Request to remove tenant with id:" + tenantId);
	outcome = confirm("Are you sure to delete the tenant with id: "+tenantId);
	if(outcome != true)
		return;
	tenant = findTenant(tenantId);
	if(tenant == null){
		alert("Internal error occured , please reload page");
		return;
	}
	
	tenant.removeTenant = true;
	var url = SERVER_URL + "/InstGenesys/rest/InstRest/UpdateVer13";
	$.ajax({
		type : 'post',
		dataType : 'json',
		// async: false,
		contentType : 'application/json; charset=utf-8',
		data : JSON.stringify(tenant),
		url : url,
		success : function(data) {
			if (data != null) {
				if (data.error) {// Error Result
					alert("Server error:"+data.errorMessage);
				} else {
					alert("Tenant for user:"+tenant.user+" has been removed for page:"+tenant.page);
				}
			}
		}

	});
}

function notifyEngine(tenantId, status) {
	console.log("Tenant ID is =" + tenantId + " turn into :" + status);
	var url = SERVER_URL + "/InstGenesys/rest/InstRest/" + status + "?id="
			+ tenantId;
	$.ajax({
		type : 'GET',
		dataType : "xml",
		url : url,
		success : function(xml) {
			var error = $(xml).find("error").text();

			if (error == true) {
				var errorMessage = $(xml).find("errorMessage");
				alert(errorMessage);

			} else
				loadTenants();
		}

	});

}
// Function to load the tenants from the server
function loadTenants() {
	// Make AJAX Call to load tenants
	var url = SERVER_URL + "/InstGenesys/rest/InstRest/Tenants";
	$.ajax({
		type : 'GET',
		dataType : "json",
		url : url,
		success : function(data) {
			$("#tabContent").empty();
			$("#tenantTabHeader").empty();
			renderTenants(data);
			tenantsData = data.tenants;
		}
	});

}

function renderTenants(data) {
	tenants = data.tenants;
	$(tenants).each(function(index) {
		tenant = tenants[index];
		buildTenantTab(tenant);
	});
}


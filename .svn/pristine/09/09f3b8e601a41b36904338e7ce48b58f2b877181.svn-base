package cxengage.facebook.conf.pojos;

public class RestAPIData {
	private String uri;
	private boolean methodGet;
	private boolean methodPost;
	private boolean inBodyData;
	private boolean inParameterData;
	private String parameterName;
	public String getUri() {
		return uri;
	}
	public void setUri(String uri) {
		this.uri = uri;
	}
	public boolean isMethodGet() {
		return methodGet;
	}
	public void setMethodGet(boolean methodGet) {
		this.methodGet = methodGet;
	}
	public boolean isMethodPost() {
		return methodPost;
	}
	public void setMethodPost(boolean methodPost) {
		this.methodPost = methodPost;
	}
	public boolean isInBodyData() {
		return inBodyData;
	}
	public void setInBodyData(boolean inBodyData) {
		this.inBodyData = inBodyData;
	}
	public boolean isInParameterData() {
		return inParameterData;
	}
	public void setInParameterData(boolean inParameterData) {
		this.inParameterData = inParameterData;
	}
	public String getParameterName() {
		return parameterName;
	}
	public void setParameterName(String parameterName) {
		this.parameterName = parameterName;
	}
	
}
